{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=development && nodemon src/index.js --variables --exec babel-node",
    "migrate:test": "knex migrate:latest   --env test --knexfile src/db/knexfile.js",
    "rollback:test": "knex migrate:rollback --env test --knexfile src/db/knexfile.js",
    "seed:test": "knex seed:run         --env test --knexfile src/db/knexfile.js",
    "reset:test": "npm run rollback:test && npm run migrate:test && npm run seed:test",
    "migrate:development": "knex migrate:latest   --env development --knexfile src/db/knexfile.js",
    "rollback:development": "knex migrate:rollback --env development --knexfile src/db/knexfile.js",
    "seed:development": "knex seed:run         --env development --knexfile src/db/knexfile.js",
    "reset:development": "npm run rollback:development && npm run migrate:development && npm run seed:development",
    "migrate:production": "knex migrate:latest   --env production --knexfile src/db/knexfile.js",
    "rollback:production": "knex migrate:rollback --env production --knexfile src/db/knexfile.js",
    "reset:production": "npm run rollback:production && npm run migrate:production && npm run seed:production",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha src/test --timeout 20000 --compilers js:babel-core/register"
  },
  "author": "tedigc",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.5.3",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "bcrypt": "^0.8.7",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.3",
    "chai": "^3.5.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsonwebtoken": "^7.1.9",
    "jwt-decode": "^2.2.0",
    "knex": "^0.12.6",
    "lodash": "^4.16.6",
    "mocha": "^3.2.0",
    "moment": "^2.18.0",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "pg": "^6.1.0",
    "validator": "^6.1.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "npm": "^3.10.9"
  }
}
